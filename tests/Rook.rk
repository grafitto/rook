let test := 10;
let Factorial := {:(num)
  if num == 0 then 1 else num * Factorial(num - 1);
}

let Printer := {:(stop)
  Print(Factorial(stop));
  if stop == 1 then Null else Printer(stop + 1.Negate);
}

